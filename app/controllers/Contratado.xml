  <businessRules>
    <rule id="r102" commandName="New" type="Sql" phase="Execute"><![CDATA[SET @Candidato_Id = 0
SET @Estatus_Contratado_Id=1
SET @Vigencia_Contrato = DateAdd(day,30,DateAdd(month,11,DateAdd(year,8099,'01/01/1900')))
SET @Acceso_App_Por_Kiosko = 0

SELECT
	@Candidato_Id = c.Candidato_Id
	,@Vacante = v.Vacante
FROM
	APP.Candidato c
	,APP.Vacante v
WHERE
	c.Candidato_Id = 0
	AND v.Vacante_Id = 0]]></rule>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT
DECLARE @Puesto_Asignado BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Contratado
	WHERE
		Persona_Id = @Persona_Id
  		AND Contratante_Id = @Contratante_Id
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya esta registrado este contratado en la base de datos, favor de corregir.'
   SET @Result_ShowAlert =  'Ya esta registrado este contratado en la base de datos, favor de corregir.'
   RETURN
END

SET @Puesto_Asignado = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Contratado c
  		,APP.Estatus_Contratado ec
	WHERE
		c.Contratante_Puesto_Id = @Contratante_Puesto_Id
  		AND c.Estatus_Registro_Id = 1
  		AND ec.Estatus_Contratado_Id = c.Estatus_Contratado_Id
  		AND (
			ec.Significa_Activo = 1
  			OR ec.Significa_Incapacitado  = 1
  			OR ec.Significa_Con_Permiso_Ausencia_Indefinida = 1
		)
),0)

IF @Puesto_Asignado=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya esta asignado este puesto a otro contratado, favor de corregir.'
   SET @Result_ShowAlert =  'Ya esta asignado este puesto a otro contratado, favor de corregir.'
   RETURN
END


