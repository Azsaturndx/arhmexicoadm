    <rule id="r101" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT
DECLARE @Puesto_Asignado BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT 
		TOP 1 1
	FROM
		APP.Contratado
	WHERE
		Persona_Id = @Persona_Id
  		AND Contratante_Id = @Contratante_Id
