    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Contratado"."Contratado_Id" "Contratado_Id"
	,"Contratado"."Contratante_Id" "Contratante_Id"
	,"Contratante_"."Contratante" "Contratante_Contratante"
	,"Contratado"."Persona_Id" "Persona_Id"
	,"Persona"."Numero_Imss" "PersonanumeroImss"
	,"Persona"."Curp_Id" "PersonacurpId"
	,"Persona"."Registro_Federal_Causante_Id" "PersonaregistroFederalCausanteId"
	,"Persona"."Fecha_Nacimiento" "PersonafechaNacimiento"
	,"Persona"."Picture" "PersonaPicture"
	,"Grid_Contacto_V"."Contacto" "Persona_Contacto_Etiqueta"
	,"Grid_Contacto_V"."Contacto_Id" "Persona_Contacto_iD"
	,"Grid_Persona_Domicilio_V"."Domicilio_Etiqueta" "Persona_Domicilio_Etiqueta"
	,"Grid_Persona_Domicilio_V"."Domicilio_Id" "Persona_Domicilio_Etiqueta_Id"
	,"Contratado"."Candidato_Id" "Candidato_Id"
	,"Contratado"."Identificador_Unico" "Identificador_Unico"
	,"Contratado"."Tipo_Contratado_Id" "Tipo_Contratado_Id"
	,"Tipo_Contratado_"."Tipo_Contratado" "Tipo_Contratado_Tipo_Contratado"
	,"Contratado"."Tipo_Empleado_Id" "Tipo_Empleado_Id"
	,"Tipo_Empleado_"."Tipo_Empleado" "Tipo_Empleado_Tipo_Empleado"
	,"Contratado"."Contratante_Estructura_Organizacional_Id" "Contratante_Estructura_Organizacional_Id"
	,"Contratante_Estructura_Organizacional_"."Estructura_Organizacional" "Contratante_Estructura_Organizacional_Estructura_Organizacional"
	,"Contratado"."Contratante_Puesto_Id" "Contratante_Puesto_Id"
	,"Contratante_Puesto_"."Contratante_Puesto" "Contratante_Puesto_Contratante_Puesto"
	,"Contratado"."Turno_Laboral_Id" "Turno_Laboral_Id"
	,"Turno_Laboral_"."Turno_Laboral" "Turno_Laboral_Turno_Laboral"
	,"Contratado"."Fecha_Contratacion" "Fecha_Contratacion"
	,"Contratado"."Fecha_Antiguedad" "Fecha_Antiguedad"
	,"Contratado"."Vigencia_Contrato" "Vigencia_Contrato"
	,"Contratado"."Documento_Contrato_Id" "Documento_Contrato_Id"
	,"Documento_Contrato_"."Documento" "Documento_Contrato_Documento"
	,"Contratado"."Notas_Contratacion" "Notas_Contratacion"
	,"Contratado"."Estatus_Contratado_Id" "Estatus_Contratado_Id"
	,"Estatus_Contratado_"."Estatus_Contratado" "Estatus_Contratado_Estatus_Contratado"
	,"Contratado"."Contratado_Key" "Contratado_Key"
	,"Contratado"."Cliente_Id" "Cliente_Id"
	,"Usuario_V"."UserName" "cpUserName"
	,"Contratado"."Fecha_Creacion" "Fecha_Creacion"
	,"Usuario_V1"."UserName" "apUserName"
	,"Contratado"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Contratado"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
	,"Vacante"."Vacante" "Vacante"
	,"Contratado"."Nivel_Organizacional_Id" "Nivel_Organizacional_Id"
	,"Nivel_Organizacional"."Nivel_Organizacional" "NivelOrganizacionalnivelOrganizacional"
	,"Contratado"."Identificador_Unico_A" "Identificador_Unico_A"
	,"Contratado"."Acceso_App_Por_Kiosko" "Acceso_App_Por_Kiosko"
	,"Genero"."Genero" "Genero"
	,"Estado_Civil"."Estado_Civil" "EstadoCivilestadoCivil"
	,"Estatus_Persona"."Estatus_Persona" "EstatusPersonaestatusPersona"
	,"Nivel_Escolaridad"."Nivel_Escolaridad" "NivelEscolaridadnivelEscolaridad"
	,"Ciudad"."Ciudad" "Ciudad"
from "APP"."Contratado" "Contratado"
	left join "APP"."Contratante" "Contratante_" on "Contratado"."Contratante_Id" = "Contratante_"."Contratante_Id"
	left join "APP"."Tipo_Contratado" "Tipo_Contratado_" on "Contratado"."Tipo_Contratado_Id" = "Tipo_Contratado_"."Tipo_Contratado_Id"
	left join "APP"."Tipo_Empleado" "Tipo_Empleado_" on "Contratado"."Tipo_Empleado_Id" = "Tipo_Empleado_"."Tipo_Empleado_Id"
	left join "APP"."Contratante_Estructura_Organizacional" "Contratante_Estructura_Organizacional_" on "Contratado"."Persona_Id" = "Contratante_Estructura_Organizacional_"."Tipo_Estructura_Organizacional_Id"
	left join "APP"."Contratante_Puesto" "Contratante_Puesto_" on "Contratado"."Contratante_Puesto_Id" = "Contratante_Puesto_"."Contratante_Puesto_Id"
	left join "APP"."Turno_Laboral" "Turno_Laboral_" on "Contratado"."Turno_Laboral_Id" = "Turno_Laboral_"."Turno_Laboral_Id"
	left join "APP"."Documento" "Documento_Contrato_" on "Contratado"."Documento_Contrato_Id" = "Documento_Contrato_"."Documento_Id"
	left join "APP"."Estatus_Contratado" "Estatus_Contratado_" on "Contratado"."Estatus_Contratado_Id" = "Estatus_Contratado_"."Estatus_Contratado_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Contratado"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Usuario_V" on "Contratado"."Creado_Por_Id" = "Usuario_V"."UserId"
	left join "dbo"."Usuario_V" "Usuario_V1" on "Contratado"."Actualizado_Por_Id" = "Usuario_V1"."UserId"
	left join "APP"."Candidato" "Candidato" on "Contratado"."Persona_Id" = "Candidato"."Candidato_Id"
	left join "APP"."Vacante" "Vacante" on "Candidato"."Vacante_Id" = "Vacante"."Vacante_Id"
	left join "dbo"."Grid_Persona_Domicilio_V" "Grid_Persona_Domicilio_V" on "Contratado"."Persona_Id" = "Grid_Persona_Domicilio_V"."Persona_Id"
	left join "dbo"."Grid_Contacto_V" "Grid_Contacto_V" on "Contratado"."Persona_Id" = "Grid_Contacto_V"."Persona_Id"
	left join "APP"."Nivel_Organizacional" "Nivel_Organizacional" on "Contratado"."Nivel_Organizacional_Id" = "Nivel_Organizacional"."Nivel_Organizacional_Id"
	left join "APP"."Persona" "Persona" on "Contratado"."Persona_Id" = "Persona"."Persona_Id"
	left join "APP"."Genero" "Genero" on "Persona"."Genero_Persona_Id" = "Genero"."Genero_Id"
	left join "APP"."Estado_Civil" "Estado_Civil" on "Persona"."Estado_Civil_Id" = "Estado_Civil"."Estado_Civil_Id"
	left join "APP"."Estatus_Persona" "Estatus_Persona" on "Persona"."Estatus_Persona_Id" = "Estatus_Persona"."Estatus_Persona_Id"
	left join "APP"."Nivel_Escolaridad" "Nivel_Escolaridad" on "Persona"."Maximo_Nivel_Escolaridad_Id" = "Nivel_Escolaridad"."Nivel_Escolaridad_Id"
	left join "APP"."Ciudad" "Ciudad" on "Persona"."Lugar_Nacimiento_Id" = "Ciudad"."Ciudad_Id"
]]></text>
    </command>
