  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Tipo_Gasto
	WHERE
		REPLACE(UPPER(Tipo_Gasto),' ','') = REPLACE(UPPER(@Tipo_Gasto),' ','')
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya esta registrado este tipo de gasto en la base de datos, favor de corregir.'
   SET @Result_ShowAlert =  'Ya esta registrado este tipo de gasto en la base de datos, favor de corregir.'
   RETURN
END

]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE
	APP.Tipo_Gasto
SET
	Creado_Por_Id = @BusinessRules_UserId
	,Fecha_Creacion = GetDate()
	,Fecha_Ultima_Actualizacion = GetDate()
	,Actualizado_Por_Id = @BusinessRules_UserId
WHERE
	Tipo_Gasto_Id= @Tipo_Gasto_Id]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT 
		TOP 1 1
	FROM
		APP.Tipo_Gasto
	WHERE
  		REPLACE(UPPER(Tipo_Gasto),' ','') = REPLACE(UPPER(@Tipo_Gasto),' ','')
		AND Tipo_Gasto_Id <> @Tipo_Gasto_Id
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya esta registrado este tipo de gasto en la base de datos, favor de corregir.'
   SET @Result_ShowAlert = 'Ya esta registrado este tipo de gasto en la base de datos, favor de corregir.'
   RETURN
END

]]></rule>
