      ,[Fecha_Ultima_Actualizacion]
      ,[Estatus_Registro_Id]
      ,[Estatus_Registro_Estatus_Registro]
      ,[Cliente_Id]
  FROM [dbo].[Grid_Contratante_Pagadora_V]]]></text>
    </command>
