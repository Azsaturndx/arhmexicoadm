  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Periodicidad"."Periodicidad_Id" "Periodicidad_Id"
	,"Periodicidad"."Periodicidad" "Periodicidad"
	,"Periodicidad"."Divisor" "Divisor"
	,"Periodicidad"."Transaccion_Id" "Transaccion_Id"
	,"Creado_Por"."UserName" "CreadoPorusername"
	,"Periodicidad"."Fecha_Creacion" "Fecha_Creacion"
	,"Actualizado_Por"."UserName" "ActualizadoPorusername"
	,"Periodicidad"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Periodicidad"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
from "APP"."Periodicidad" "Periodicidad"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Periodicidad"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Creado_Por" on "Periodicidad"."Creado_Por_Id" = "Creado_Por"."UserId"
	left join "dbo"."Usuario_V" "Actualizado_Por" on "Periodicidad"."Actualizado_Por_Id" = "Actualizado_Por"."UserId"
]]></text>
    </command>
    <command id="Periodicidad_IdIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="Periodicidad_Id" />
      </output>
    </command>
  </commands>
