    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Nivel_Organizacional"."Nivel_Organizacional_Id" "Nivel_Organizacional_Id"
	,"Nivel_Organizacional"."Nivel_Organizacional" "Nivel_Organizacional"
	,"Nivel_Organizacional"."Es_Ejecutivo" "Es_Ejecutivo"
	,"Nivel_Organizacional"."Es_Sindicalizado_Semanal" "Es_Sindicalizado_Semanal"
	,"Nivel_Organizacional"."Es_De_Confianza_Administrativo" "Es_De_Confianza_Administrativo"
	,"Nivel_Organizacional"."Transaccion_Id" "Transaccion_Id"
	,"Usuario_V"."UserName" "cpUserName"
	,"Nivel_Organizacional"."Fecha_Creacion" "Fecha_Creacion"
	,"Usuario_V1"."UserName" "apUserName"
	,"Nivel_Organizacional"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Nivel_Organizacional"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
from "APP"."Nivel_Organizacional" "Nivel_Organizacional"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Nivel_Organizacional"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Usuario_V" on "Nivel_Organizacional"."Creado_Por_Id" = "Usuario_V"."UserId"
	left join "dbo"."Usuario_V" "Usuario_V1" on "Nivel_Organizacional"."Actualizado_Por_Id" = "Usuario_V1"."UserId"
]]></text>
    </command>
