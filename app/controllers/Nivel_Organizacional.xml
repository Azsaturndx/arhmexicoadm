  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Nivel_Organizacional
	WHERE
		REPLACE(UPPER(Nivel_Organizacional),' ','') = REPLACE(UPPER(@Nivel_Organizacional),' ','')
  
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya existe este registro de nivel organizacional en la base de datos, favor de corregir.'
   SET @Result_ShowAlert =  'Ya existe este registro de nivel organizacional en la base de datos, favor de corregir.'
   RETURN
END
]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE
	APP.Nivel_Organizacional
SET
	Creado_Por_Id = @BusinessRules_UserId
	,Actualizado_Por_Id = @BusinessRules_UserId
	,Fecha_Ultima_Actualizacion = GetDate()
	,Fecha_Creacion = GetDate()
WHERE
	Nivel_Organizacional_Id= @Nivel_Organizacional_Id]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT 
		TOP 1 1
	FROM
		APP.Nivel_Organizacional
	WHERE
  		REPLACE(UPPER(Nivel_Organizacional),' ','') = REPLACE(UPPER(@Nivel_Organizacional),' ','')
		AND Nivel_Organizacional_Id <> @Nivel_Organizacional_Id
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya existe este registro de nivel organizacional en la base de datos, favor de corregir.'
   SET @Result_ShowAlert = 'Ya existe este registro de nivel organizacional en la base de datos, favor de corregir.'
   RETURN
END





]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="After"><![CDATA[UPDATE
