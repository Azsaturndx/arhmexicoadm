  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT


SET @Existe = 0

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Contratante
	WHERE
		REPLACE(UPPER(Contratante),' ','') = REPLACE(UPPER(@Contratante),' ','')
),0)

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Contratante
	WHERE
		REPLACE(UPPER(Clave_Contratante),' ','') = REPLACE(UPPER(@Clave_Contratante),' ','')
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya esta registrado este NOMBRE de Contratante ó CLAVE DE CONTRATANTE en la base de datos, favor de corregir.'
   SET @Result_ShowAlert =  'Ya esta registrado este NOMBRE de Contratante ó CLAVE DE CONTRATANTE en la base de datos, favor de corregir.'
   RETURN
END


IF(@Vigencia_Dias<1)
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'No puede elegir ese número de días como vigencia de recibos de nómina'
   SET @Result_ShowAlert =  'No puede elegir ese número de días como vigencia de recibos de nómina'
   RETURN
END

IF(@Vigencia_Dias>99999)
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'No puede elegir ese número de días como vigencia de recibos de nómina, debe elegir un número entre 1 y 99,999'
   SET @Result_ShowAlert =  'No puede elegir ese número de días como vigencia de recibos de nómina, debe elegir un número entre 1 y 99,999'
   RETURN
END
]]></rule>
