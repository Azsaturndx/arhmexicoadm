    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Contratante"."Contratante_Id" "Contratante_Id"
	,"Contratante"."Contratante" "Contratante"
	,"Contratante"."Contratante_Razon_Social" "Contratante_Razon_Social"
	,"Contratante"."Contratante_Descripcion" "Contratante_Descripcion"
	,"Contratante"."Cliente_Id" "Cliente_Id"
	,"Cliente_"."Cliente" "Cliente_Cliente"
	,"Contratante"."Transaccion_Id" "Transaccion_Id"
	,"Contratante"."Creado_Por_Id" "Creado_Por_Id"
	,"Usuario_V"."UserName" "cpUserName"
	,"Contratante"."Fecha_Creacion" "Fecha_Creacion"
	,"Contratante"."Actualizado_Por_Id" "Actualizado_Por_Id"
	,"Usuario_V1"."UserName" "apUserName"
	,"Contratante"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Contratante"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
	,"Persona_V"."Persona_Nombre" "Persona_Nombre"
	,"Contratante"."Contratado_Encargado_RH_Id" "Contratado_Encargado_RH_Id"
	,"Contratante"."Clave_Contratante" "Clave_Contratante"
	,"Contratante"."Vigencia_Dias" "Vigencia_Dias"
	,"Contratante"."Nombre_Dominio" "Nombre_Dominio"
	,"Contratante"."Mostrar_Religion_A_Aspirante" "Mostrar_Religion_A_Aspirante"
	,"Contratante"."Mostrar_Orientacion_Sexual_A_Aspirante" "Mostrar_Orientacion_Sexual_A_Aspirante"
	,"Contratante"."Mostrar_Senas_Particulares_A_Aspirante" "Mostrar_Senas_Particulares_A_Aspirante"
	,"Contratante"."Permite_Cancelar_Solicitud_Vacaciones" "Permite_Cancelar_Solicitud_Vacaciones"
	,"Contratante"."Layout_Correo_Electronico_Contratado_Id" "Layout_Correo_Electronico_Contratado_Id"
	,"Layout_Correo_Electronico"."Layout_Correo_Electronico" "LayoutCorreoElectronicoContratado"
	,"Contratante"."Layout_Correo_Electronico_Jefe_Id" "Layout_Correo_Electronico_Jefe_Id"
	,"Layout_Correo_Electronico1"."Layout_Correo_Electronico" "LayoutCorreoElectronicoJefe"
	,"Contratante"."Layout_Correo_Electronico_RH_Respuesta_Solicitud_Vacaciones_Id" "Layout_Correo_Electronico_RH_Respuesta_Solicitud_Vacaciones_Id"
	,"Layout_Correo_Electronico3"."Layout_Correo_Electronico" "LayoutCorreoElectronicoRHRespuestaSolicitudVacaciones"
	,"Contratante"."Layout_Correo_Electronico_RH_Solicitud_Vacaciones_Id" "Layout_Correo_Electronico_RH_Solicitud_Vacaciones_Id"
	,"Layout_Correo_Electronico2"."Layout_Correo_Electronico" "LayoutCorreoElectronico2layoutCorreoElectronico"
	,"Contratante"."Generar_Usuario_Generico" "GenerarUsuarioGenerico"
	,"Contratante"."Permitir_Solicitar_Gastos" "PermitirSolicitarGastos"
	,"Contratante"."Numero_Telefono" "NumeroTelefono"
	,"Contratante"."Permite_Enviar_Correo_Recibo_Nomina" "PermiteEnviarCorreoReciboNomina"
	,"Contratante"."Permite_Solicitar_Prestamos" "PermiteSolicitarPrestamos"
	,"Contratante"."Contratado_Encargado_Prestamos_Id" "ContratadoEncargadoPrestamosId"
	,"Contratante"."Antiguedad_Minima_Pedir_Prestamo" "AntiguedadMinimaPedirPrestamo"
	,"Contratante"."Layout_Correo_Electronico_Contratado_Solicitud_Prestamo_Id" "LayoutCorreoElectronicoContratadoSolicitudPrestamoId"
	,"Contratante"."Layout_Correo_Electronico_Encargado_Prestamo_Id" "LayoutCorreoElectronicoEncargadoPrestamoId"
	,"Contratante"."Layout_Correo_Electronico_RH_Solicitud_Prestamo_Id" "LayoutCorreoElectronicoRHSolicitudPrestamoId"
	,"Contratante"."Layout_Correo_Electronico_RH_Respuesta_Solicitud_Prestamo_Id" "LayoutCorreoElectronicoRHRespuestaSolicitudPrestamoId"
	,"Contratante"."Layout_Correo_Electronico_Jefe_Notificacion_Cancelacion_Id" "LayoutCorreoElectronicoJefeNotificacionCancelacionId"
	,"Contratante"."Validar_Vacaciones_Al_Pasado" "ValidarVacacionesAlPasado"
	,"Contratante"."Validar_Vacaciones_Dia_Actual" "ValidarVacacionesDiaActual"
from "APP"."Contratante" "Contratante"
	left join "APP"."Cliente" "Cliente_" on "Contratante"."Cliente_Id" = "Cliente_"."Cliente_Id"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Contratante"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "Usuario_V" on "Contratante"."Creado_Por_Id" = "Usuario_V"."UserId"
	left join "dbo"."Usuario_V" "Usuario_V1" on "Contratante"."Actualizado_Por_Id" = "Usuario_V1"."UserId"
	left join "APP"."Contratado" "Contratado" on "Contratante"."Contratado_Encargado_RH_Id" = "Contratado"."Contratado_Id"
	left join "dbo"."Persona_V" "Persona_V" on "Contratado"."Contratado_Id" = "Persona_V"."Contratado_Id"
	left join "APP"."Layout_Correo_Electronico" "Layout_Correo_Electronico" on "Contratante"."Layout_Correo_Electronico_Contratado_Id" = "Layout_Correo_Electronico"."Layout_Correo_Electronico_Id"
	left join "APP"."Layout_Correo_Electronico" "Layout_Correo_Electronico1" on "Contratante"."Layout_Correo_Electronico_Jefe_Id" = "Layout_Correo_Electronico1"."Layout_Correo_Electronico_Id"
	left join "APP"."Layout_Correo_Electronico" "Layout_Correo_Electronico2" on "Contratante"."Layout_Correo_Electronico_RH_Solicitud_Vacaciones_Id" = "Layout_Correo_Electronico2"."Layout_Correo_Electronico_Id"
	left join "APP"."Layout_Correo_Electronico" "Layout_Correo_Electronico3" on "Contratante"."Layout_Correo_Electronico_RH_Respuesta_Solicitud_Vacaciones_Id" = "Layout_Correo_Electronico3"."Layout_Correo_Electronico_Id"
]]></text>
    </command>
