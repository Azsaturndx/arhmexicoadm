	,"Gastos_Frecuentes"."Fecha_Creacion" "Fecha_Creacion"
	,"Gastos_Frecuentes"."Actualizado_Por_Id" "Actualizado_Por_Id"
	,"apUsuario_V"."UserName" "apUserName"
