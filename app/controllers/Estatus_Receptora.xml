	,Fecha_Creacion = GetDate()
	,Fecha_Ultima_Actualizacion = GetDate()
	,Actualizado_Por_Id = @BusinessRules_UserId
WHERE
	Estatus_Receptora_Id= @Estatus_Receptora_Id]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Estatus_Receptora
	WHERE
		REPLACE(UPPER(Estatus_Receptora),' ','') = REPLACE(UPPER(@Estatus_Receptora),' ','')
  		AND Estatus_Receptora_Id <> @Estatus_Receptora_Id
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya esta registrado este estatus de receptora en la base de datos, favor de corregir.'
   SET @Result_ShowAlert =  'Ya esta registrado este estatus de receptora en la base de datos, favor de corregir.'
   RETURN
END

]]></rule>
