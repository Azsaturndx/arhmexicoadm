    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Estatus_Receptora"."Estatus_Receptora_Id" "Estatus_Receptora_Id"
	,"Estatus_Receptora"."Estatus_Receptora" "Estatus_Receptora"
	,"Estatus_Receptora"."Transaccion_Id" "Transaccion_Id"
	,"Estatus_Receptora"."Creado_Por_Id" "Creado_Por_Id"
	,"cpUsuario_V"."UserName" "cpUserName"
	,"Estatus_Receptora"."Fecha_Creacion" "Fecha_Creacion"
	,"Estatus_Receptora"."Actualizado_Por_Id" "Actualizado_Por_Id"
	,"apUsuario_V"."UserName" "apUserName"
	,"Estatus_Receptora"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Estatus_Receptora"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
from "APP"."Estatus_Receptora" "Estatus_Receptora"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Estatus_Receptora"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "cpUsuario_V" on "Estatus_Receptora"."Creado_Por_Id" = "cpUsuario_V"."UserId"
	left join "dbo"."Usuario_V" "apUsuario_V" on "Estatus_Receptora"."Actualizado_Por_Id" = "apUsuario_V"."UserId"
]]></text>
    </command>
