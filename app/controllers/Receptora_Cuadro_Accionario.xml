from "APP"."Receptora_Cuadro_Accionario" "Receptora_Cuadro_Accionario"
	left join "APP"."Receptora" "Receptora_" on "Receptora_Cuadro_Accionario"."Receptora_Id" = "Receptora_"."Receptora_Id"
