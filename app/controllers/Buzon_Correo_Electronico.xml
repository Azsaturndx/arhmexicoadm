	left join "dbo"."Usuario_V" "Usuario_V" on "Buzon_Correo_Electronico"."Creado_Por_Id" = "Usuario_V"."UserId"
	left join "dbo"."Usuario_V" "Usuario_V1" on "Buzon_Correo_Electronico"."Actualizado_Por_Id" = "Usuario_V1"."UserId"
]]></text>
