select
	"Tipo_Deposito"."Tipo_Deposito_Id" "Tipo_Deposito_Id"
	,"Tipo_Deposito"."Tipo_Deposito" "Tipo_Deposito"
	,"Tipo_Deposito"."Tipo_Deposito_Abreviado" "Tipo_Deposito_Abreviado"
	,"Tipo_Deposito"."Creado_Por_Id" "Creado_Por_Id"
	,"cpUsuario_V"."UserName" "cpUserName"
	,"Tipo_Deposito"."Fecha_Creacion" "Fecha_Creacion"
	,"Tipo_Deposito"."Actualizado_Por_Id" "Actualizado_Por_Id"
	,"apUsuario_V"."UserName" "apUserName"
	,"Tipo_Deposito"."Fecha_Ultima_Actualizacion" "Fecha_Ultima_Actualizacion"
	,"Tipo_Deposito"."Estatus_Registro_Id" "Estatus_Registro_Id"
	,"Estatus_Registro_"."Estatus_Registro" "Estatus_Registro_Estatus_Registro"
from "APP"."Tipo_Deposito" "Tipo_Deposito"
	left join "dbo"."Estatus_Registro" "Estatus_Registro_" on "Tipo_Deposito"."Estatus_Registro_Id" = "Estatus_Registro_"."Estatus_Registro_Id"
	left join "dbo"."Usuario_V" "cpUsuario_V" on "Tipo_Deposito"."Creado_Por_Id" = "cpUsuario_V"."UserId"
	left join "dbo"."Usuario_V" "apUsuario_V" on "Tipo_Deposito"."Actualizado_Por_Id" = "apUsuario_V"."UserId"
]]></text>
    </command>
