  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT
		TOP 1 1
	FROM
		APP.Tipo_Deposito
	WHERE
		REPLACE(UPPER(Tipo_Deposito),' ','') = REPLACE(UPPER(@Tipo_Deposito),' ','')
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya existe este tipo de depósito en la base de datos, favor de corregir.'
   SET @Result_ShowAlert =  'Ya existe este tipo de depósito en la base de datos, favor de corregir.'
   RETURN
END
]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="After"><![CDATA[UPDATE
	APP.Tipo_Deposito
SET
	Creado_Por_Id = @BusinessRules_UserId
	,Actualizado_Por_Id = @BusinessRules_UserId
	,Fecha_Ultima_Actualizacion = GetDate()
	,Fecha_Creacion = GetDate()
WHERE
	Tipo_Deposito_Id= @Tipo_Deposito_Id]]></rule>
    <rule id="r102" commandName="Update" type="Sql" phase="Before"><![CDATA[DECLARE @Existe BIT

SET @Existe = 0

SET @Existe = IsNULL((
	SELECT 
		TOP 1 1
	FROM
		APP.Tipo_Deposito
	WHERE
  		REPLACE(UPPER(Tipo_Deposito),' ','') = REPLACE(UPPER(@Tipo_Deposito),' ','')
		AND Tipo_Deposito_Id <> @Tipo_Deposito_Id
),0)

IF @Existe=1
BEGIN
   SET @BusinessRules_PreventDefault = 1
   SET @Result_ShowViewMessage = 'Ya existe este tipo de depósito en la base de datos, favor de corregir.'
   SET @Result_ShowAlert = 'Ya existe este tipo de depósito en la base de datos, favor de corregir.'
   RETURN
END
]]></rule>
    <rule id="r103" commandName="Update" type="Sql" phase="After"><![CDATA[UPDATE
